{
  "name": "core-watermark",
  "version": "1.0.1",
  "private": false,
  "description": "水印",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --config build/webpack.dev.js --open",
    "build": "webpack --config build/webpack.build.js",
    "start": "http-server dist",
    "commit": "npm run log && git add ./CHANGELOG.md && git cz",
    "log": "conventional-changelog --config ./node_modules/vue-cli-plugin-commitlint/lib/log -i CHANGELOG.md -s -r 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liuqiyu/watermarkjs.git"
  },
  "keywords": [
    "watermark"
  ],
  "author": "liuqiyu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liuqiyu/watermarkjs/issues"
  },
  "homepage": "https://github.com/liuqiyu/watermarkjs#readme",
  "dependencies": {},
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "eslint": "^8.10.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "commitizen": "^4.2.4",
    "commitlint": "^16.2.3",
    "conventional-changelog-cli": "^2.2.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "right-pad": "^1.0.1",
    "vue-cli-plugin-commitlint": "^1.0.12"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/vue-cli-plugin-commitlint/lib/cz"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}